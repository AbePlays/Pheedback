{
  "private": true,
  "name": "pheedback",
  "description": "",
  "license": "",
  "scripts": {
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/tailwind.css",
    "build": "npm run build:css && remix build",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "postinstall": "remix setup node",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-popover": "^0.1.6",
    "@radix-ui/react-tabs": "^0.1.5",
    "@remix-run/react": "^1.3.1",
    "@remix-run/serve": "^1.3.1",
    "@remix-run/vercel": "^1.3.1",
    "bcrypt": "5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.11.0",
    "tailwindcss": "^3.0.23",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "@types/react": "^17.0.40"
  },
  "sideEffects": false
}
